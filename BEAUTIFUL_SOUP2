from bs4 import BeautifulSoup
import requests

URL="https://www.empireonline.com/movies/features/best-movies-2"
response=requests.get(URL)
website_html=response.text
# print(movies)

soup= BeautifulSoup(website_html, "html.parser")
print(soup.prettify())
